{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.3",
  "description": "",
  "license": "MIT",
  "author": "oOBobbyOo <https://github.com/oOBobbyOo>",
  "homepage": "https://github.com/oOBobbyOo/vite-react-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oOBobbyOo/vite-react-template.git"
  },
  "bugs": {
    "url": "https://github.com/oOBobbyOo/vite-react-template/issues"
  },
  "keywords": [
    "vite",
    "react"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:test": "tsc && vite build --mode test",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode prod",
    "prepublishOnly": "pnpm build:prod",
    "release": "bumpp && pnpm publish",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:style": "stylelint --fix \"src/**/*.{css,less}\" --custom-syntax postcss-less",
    "stylelint-check": "stylelint-config-prettier-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.14.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.26",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "less": "^4.1.3",
    "lint-staged": "^12.4.0",
    "postcss": "^8.4.12",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.less": "stylelint --fix --custom-syntax postcss-less",
    "src/**/*.{js,jsx,ts.tsx}": [
      "prettier --ignore-path ./.prettierignore --config .prettierrc --write",
      "pnpm lint:fix"
    ]
  }
}
